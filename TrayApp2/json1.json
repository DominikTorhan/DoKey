    //"n.ml" "{UP 10}",
    //"n.mi" "{DOWN 20}",
    //"n.mo" "{UP 20}",
    //oemquestion

    //control
    "n.z": "^z",
    "n.r": "^y",
    "n.x": "^x",
    "n.c": "^c",
    "n.v": "^v"

  },

  //insert mode with c
  "insert mode": {
    "ic.e": "{ESC}",
    "ic.h": "{BKSP}",
    "ic.l": "{DEL}",
    "ic.j": "{ENTER}",
    "ic.t": "{TAB}",
    "ic.z": "^z"
  }

}


