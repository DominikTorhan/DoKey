  //normal mode
  "normal mode": {
    "2step": "i,n,d,f,s,m",

    //
    "n.y": "^{LEFT}",
    "n.u": "^{RIGHT}",
    //insert
    "n.ii": "{End} {ENTER}",
    "n.io": "{UP}{End}{ENTER}",
    "n.ij": "{End} {ENTER}",
    "n.ik": "{UP}{End}{ENTER}",
    "n.ih": "\" \"",
    "n.il": "\" \"",
    //select / switch
    "n.se": "%{UP}",
    "n.sd": "%{DOWN}",
    "n.ss": "{END}+{HOME}+{HOME}",
    "n.sf": "{RIGHT}^{LEFT}+^{RIGHT}",
    "n.soemcomma": "+{HOME}",
    "n.soemperiod": "+{END}",
    //delete
    "n.dd": "{END}+{HOME}+{HOME}\" \"{BKSP}{DEL}",
    "n.df": "{RIGHT}^{LEFT}+^{RIGHT}{DEL}",
    "n.dh": "{BKSP}",
    "n.dl": "{DEL}",
    "n.doemcomma": "+{HOME}{DEL}",
    "n.doemperiod": "+{END}{DEL}",
    //func
    "n.fd1": "{F1}",
    "n.fd2": "{F2}",
    "n.fd3": "{F3}",
    "n.fd4": "{F4}",
    "n.fd5": "{F5}",
    "n.fd6": "{F6}",
    "n.fq": "{F7}",
    "n.fw": "{F8}",
    "n.fe": "{F9}",
    "n.fr": "{F10}",
    "n.ft": "{F11}",
    "n.ff": "{F12}",

    //arrows
    "n.h": "{LEFT}",
    "n.j": "{DOWN}",
    "n.k": "{UP}",
    "n.l": "{RIGHT}",
    //navigate
    "n.nn": "%{TAB}",
    "n.ni": "^+{TAB}",
    "n.no": "^{TAB}",
    "n.nk": "^{PGUP}",
    "n.nl": "^{PGDN}",
    "n.noemcomma": "%{LEFT}",
    "n.noemperiod": "%{RIGHT}",
    //move

    //"m.n" "{PGUP}",
    "n.mj": "{PGDN}",
    "n.mk": "{PGUP}",
    "n.mh": "^{HOME}",
    "n.mn": "^{END}",
    "n.oemcomma": "{HOME}",
    "n.oemperiod": "{END}",
    "n.oemquestion": "^f",
    "n.mm": "^m^m",
    "n.ma": "^m^a",
    "n.mo": "^m^o",
    "n.me": "^+{F12}",
